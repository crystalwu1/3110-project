### Starting build.
# Target: main.ml.depends, tags: { package(graphics), extension:ml, file:main.ml, ocaml, ocamldep, quiet, traverse }
ocamlfind ocamldep -package graphics -modules main.ml > main.ml.depends
# Target: board.ml.depends, tags: { package(graphics), extension:ml, file:board.ml, ocaml, ocamldep, quiet, traverse }
ocamlfind ocamldep -package graphics -modules board.ml > board.ml.depends
# Target: board.cmo, tags: { package(graphics), byte, compile, extension:cmo, extension:ml, file:board.cmo, file:board.ml, implem, ocaml, quiet, traverse }
ocamlfind ocamlc -c -package graphics -o board.cmo board.ml
# Target: main.cmo, tags: { package(graphics), byte, compile, extension:cmo, extension:ml, file:main.cmo, file:main.ml, implem, ocaml, quiet, traverse }
ocamlfind ocamlc -c -package graphics -o main.cmo main.ml
# Target: main.byte, tags: { package(graphics), byte, dont_link_with, extension:byte, file:main.byte, link, ocaml, program, quiet, traverse }
ocamlfind ocamlc -linkpkg -package graphics board.cmo main.cmo -o main.byte
# Compilation successful.
